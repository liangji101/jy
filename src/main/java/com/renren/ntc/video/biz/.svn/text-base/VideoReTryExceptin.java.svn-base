package com.renren.ntc.video.biz;

/**
 * 视频运行时异常
 * 
 * @author gaofeng@renren-inc.com @since 2012-3-31
 */
public class VideoReTryExceptin extends RuntimeException {

    public VideoReTryExceptin(String msg) {
        super(msg);
    }

    public VideoReTryExceptin(Throwable e) {
        super(e);
    }

    public VideoReTryExceptin(String msg, Throwable e) {
        super(msg, e);
    }

    private static final long serialVersionUID = -4738221021866945695L;

}
