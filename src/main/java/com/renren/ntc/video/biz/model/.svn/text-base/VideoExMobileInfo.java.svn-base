package com.renren.ntc.video.biz.model;

import org.apache.commons.lang.StringUtils;

/**
 * 3G视频上传额外信息
 * 
 * @author gaofeng@renren-inc.com @since 2012-3-31
 */
public class VideoExMobileInfo {

    /** 视频MD5值 **/
    private String md5;

    /** 机器码，通常选择手机的imei码 **/
    private String machineID;

    /** 机器型号信息，如htc哪个型号 **/
    private String machineInfo;

    /** 源视频文件名 **/
    private String vFile;

    /** 源视频文件扩展名 **/
    private String extName;

    /** 源视频文件大小 **/
    private int srcFileSize;

    /** 是否同源，2为同源，1为直接上传 **/
    private String transType;

    /** 视频画质，0:normal, 1:clear, 2:super **/
    private String videoType;

    public VideoExMobileInfo(String md5, String machineID, String machineInfo, String vFile, String extName, int srcFileSize, String transType,
            String videoType) {
        super();
        this.md5 = md5;
        this.machineID = machineID;
        this.machineInfo = machineInfo;
        this.vFile = vFile;
        this.extName = extName;
        this.srcFileSize = srcFileSize;
        this.transType = transType;
        this.videoType = videoType;
    }

    public String getMd5() {
        return md5;
    }

    public void setMd5(String md5) {
        this.md5 = md5;
    }

    public String getMachineID() {
        return machineID;
    }

    public void setMachineID(String machineID) {
        this.machineID = machineID;
    }

    public String getMachineInfo() {
        return machineInfo;
    }

    public void setMachineInfo(String machineInfo) {
        this.machineInfo = machineInfo;
    }

    public String getvFile() {
        return vFile;
    }

    public void setvFile(String vFile) {
        this.vFile = vFile;
    }

    public String getExtName() {
        return extName;
    }

    public void setExtName(String extName) {
        this.extName = extName;
    }

    public int getSrcFileSize() {
        return srcFileSize;
    }

    public void setSrcFileSize(int srcFileSize) {
        this.srcFileSize = srcFileSize;
    }

    public String getTransType() {
        return transType;
    }

    public void setTransType(String transType) {
        this.transType = transType;
    }

    public String getVideoType() {
        return videoType;
    }

    public void setVideoType(String videoType) {
        this.videoType = videoType;
    }

    /**
     * 验证参数是否为空或合法
     * 
     * @return
     */
    public boolean validate() {
        if (StringUtils.isEmpty(md5) || StringUtils.isEmpty(machineID) || StringUtils.isEmpty(machineInfo) || StringUtils.isEmpty(vFile)
                || StringUtils.isEmpty(extName) || StringUtils.isEmpty(transType) || StringUtils.isEmpty(videoType)) {
            return false;
        }
        return true;
    }

    /**
     * Constructs a <code>String</code> with all attributes in name = value
     * format.
     * 
     * @return a <code>String</code> representation of this object.
     */
    public String toString() {
        final String TAB = "    ";
        String retValue = "";
        retValue = "VideoExMobileInfo ( " + super.toString() + TAB + "md5 = " + this.md5 + TAB + "machineID = " + this.machineID + TAB
                + "machineInfo = " + this.machineInfo + TAB + "vFile = " + this.vFile + TAB + "extName = " + this.extName + TAB + "srcFileSize = "
                + this.srcFileSize + TAB + "transType = " + this.transType + TAB + "videoType = " + this.videoType + TAB + " )";
        return retValue;
    }

}
