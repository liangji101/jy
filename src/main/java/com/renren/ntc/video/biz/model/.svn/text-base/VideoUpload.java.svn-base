package com.renren.ntc.video.biz.model;

import java.util.Date;

/**
 * @author zhancheng.deng
 * @since 2011.08.15 20:06:00
 * 
 * <br>视频上传类
 * <br>用于上传视频时，视频信息{@linkplain VideoInfo}与用户在保存视频之前的关系，可以想当于一张关系临时表
 * <br>用户保存后的关系，基本上都在视频{@linkplain Video}里面
 */
public class VideoUpload {

	/**
	 * sequence id
	 */
	private long id;

	/**
	 * 请求的用户Id
	 */
	private int userId;
	/**
	 * == UUID
	 */
	private String itemCode;
	/**
	 * 用户客户端的IP
	 */
	private String remoteAddress;
	/**
	 *  请求上传的时间
	 */
	private Date requestTime;

	/**
	 * 上传方式（手机上传，还是web上传）
	 */
	private int wap;

	/**
	 * 上传文件名
	 */
	private String fileName;
	/**
	 * 扩展名
	 */
	private String fileExtension;

	/**
	 * 视频信息的主键
	 */
	private long videoInfoId;

	/**
	 * 是{@linkplain VideoInfo} 主键
	 */
	private String checksum;  
	
	/**
	 * 用来备注
	 */
	private String comment;
	
	
	
	
	/**
	 * 默认构造器
	 */
	public VideoUpload(){
		
	}
	
	/**
	 *  常用构造器
	 * @param id
	 * @param userId
	 * @param itemCode
	 * @param remoteAddress
	 * @param requestTime
	 * @param checksum
	 */
	public VideoUpload(
			long id, 
			int userId, 
			String itemCode,
			long videoInfoId,
			String checksum, //
			String remoteAddress, 
			Date requestTime){
		this.id = id;
		this.userId = userId;
		this.itemCode = itemCode;
		
		this.videoInfoId = videoInfoId;
		this.checksum = checksum;
		
		this.remoteAddress = remoteAddress;
		this.requestTime = requestTime;
	};
	
	/**
	 *  完全构造器
	 * @param id
	 * @param userId
	 * @param itemCode
	 * @param videoInfoId
	 * @param checksum
	 * @param remoteAddress
	 * @param requestTime
	 * @param fileName
	 * @param fileExtension
	 * @param comment
	 */
	public VideoUpload( //
			long id, //
			
			int userId, //
			String itemCode, //
			
			long videoInfoId,
			String checksum, //
			
			String remoteAddress, //
			Date requestTime, //
			
			int wap, //
			
			String fileName, //
			String fileExtension, //
			String comment){
		this.id = id;
		
		this.userId = userId;
		this.itemCode = itemCode;
		
		this.videoInfoId = videoInfoId;
		this.checksum = checksum;
		
		this.remoteAddress = remoteAddress;
		this.requestTime = requestTime;
		
		this.wap = wap;
		
		this.fileName = fileName;
		this.fileExtension = fileExtension;
		this.comment = comment;
	};
	
	public long getId() {
		return id;
	}

	public void setId(long id) {
		this.id = id;
	}
	
	public int getUserId() {
		return userId;
	}
	public void setUserId(int userId) {
		this.userId = userId;
	}
	public String getItemCode() {
		return itemCode;
	}
	public void setItemCode(String itemCode) {
		this.itemCode = itemCode;
	}
	public String getRemoteAddress() {
		return remoteAddress;
	}
	public void setRemoteAddress(String remoteAddress) {
		this.remoteAddress = remoteAddress;
	}
	public Date getRequestTime() {
		return requestTime;
	}
	public void setRequestTime(Date requestTime) {
		this.requestTime = requestTime;
	}

	public String getFileName() {
		return fileName;
	}

	public void setFileName(String fileName) {
		this.fileName = fileName;
	}

	public String getFileExtension() {
		return fileExtension;
	}

	public void setFileExtension(String fileExtension) {
		this.fileExtension = fileExtension;
	}


	public void setVideoInfoId(long videoInfoId) {
		this.videoInfoId = videoInfoId;
	}

	public long getVideoInfoId() {
		return videoInfoId;
	}

	public void setChecksum(String checksum) {
		this.checksum = checksum;
	}


	public String getChecksum() {
		return checksum;
	}

	public String getComment() {
		return comment;
	}

	public void setComment(String comment) {
		this.comment = comment;
	}

	public void setWap(int wap) {
		this.wap = wap;
	}

	public int getWap() {
		return wap;
	}
	
	
	
}
