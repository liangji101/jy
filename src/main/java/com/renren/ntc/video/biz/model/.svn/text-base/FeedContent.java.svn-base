package com.renren.ntc.video.biz.model;

import com.renren.ntc.video.utils.FeedType;

public class FeedContent extends Content {
	
	private  String title;
	private  String input;
	
	public FeedContent(String title, String input){
		this.title = title;
		this.input = input;
	}
	
    public void setTitle(String title){
    	this.title = title;
    }
    
    public String getTitle( ){
    	return title;
    }
    
    public void setInput(String input){
    	this.input = input;
    }
    
    public String getInput( ){
    	return input;
    }
    
    
    public FeedType getFeedType(){
  	  return FeedType.COMMON;
    }     
 
}
