public class AntiSpamValidationException extends Exception {

    public AntiSpamValidationException() {
    }

    public AntiSpamValidationException(String message) {
        super(message);
    }

    public AntiSpamValidationException(String message, Throwable cause) {
        super(message, cause);
    }

    public AntiSpamValidationException(Throwable cause) {
        super(cause);
    }
}
